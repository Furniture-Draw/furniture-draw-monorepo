{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@furniture-draw/about-page": ["libs/about-page/src/index.ts"],
      "@furniture-draw/about-page/server": ["libs/about-page/src/server.ts"],
      "@furniture-draw/blog": ["libs/blog/src/index.ts"],
      "@furniture-draw/contact": ["libs/contact/src/index.ts"],
      "@furniture-draw/features": ["libs/features/src/index.ts"],
      "@furniture-draw/footer": ["libs/footer/src/index.ts"],
      "@furniture-draw/hero": ["libs/hero/src/index.ts"],
      "@furniture-draw/login-register-page": [
        "libs/login-register-page/src/index.ts"
      ],
      "@furniture-draw/navigation": ["libs/navigation/src/index.ts"],
      "@furniture-draw/pricing-page": ["libs/pricing-page/src/index.ts"],
      "@furniture-draw/pricingCards": ["libs/pricingCards/src/index.ts"],
      "@furniture-draw/register": ["libs/register/src/index.ts"],
      "@furniture-draw/resetpassword": ["libs/resetpassword/src/index.ts"],
      "@furniture-draw/sponsors": ["libs/sponsors/src/index.ts"],
      "@furniture-draw/testimonial": ["libs/testimonial/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
